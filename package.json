{
  "name": "@karinjs/adapter-icqq",
  "version": "1.4.0",
  "description": "icqq adapter",
  "license": "MIT",
  "author": "karin",
  "type": "module",
  "main": "lib/index.js",
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "icqq": "node lib/tools/icqq.js",
    "pub": "npm publish --access public",
    "sort": "sort-package-json",
    "karin": "karin",
    "dev": "tsx src/index.ts"
  },
  "dependencies": {
    "icqq": "npm:@icqqjs/icqq@^1.5.12"
  },
  "devDependencies": {
    "@types/express": "latest",
    "@types/lodash": "latest",
    "@types/node": "latest",
    "@types/node-schedule": "latest",
    "@types/ws": "latest",
    "eslint": "latest",
    "neostandard": "latest",
    "node-karin": "^1.4.5",
    "tsc-alias": "latest",
    "tsx": "latest",
    "typescript": "latest"
  },
  "peerDependenciesMeta": {
    "@icqqjs/icqq": {
      "optional": true
    }
  },
  "karin": {
    "main": "src/index.ts",
    "ts-apps": [
      "src/apps"
    ],
    "apps": [
      "lib/apps"
    ],
    "outDir": "lib",
    "exclude": [
      "lib/**/*",
      "@karinjs/**/*"
    ],
    "files": [
      "config",
      "data",
      "resources"
    ],
    "include": [
      "src/**/*"
    ]
  }
}