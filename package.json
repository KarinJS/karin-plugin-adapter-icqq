{
  "name": "@karinjs/adapter-icqq",
  "version": "1.3.14",
  "description": "icqq adapter",
  "license": "MIT",
  "author": "karin",
  "type": "module",
  "main": "./lib/index.js",
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json && npm run fix",
    "fix": "eslint lib/**/*.js --fix",
    "pub": "npm publish --access public",
    "sort": "sort-package-json",
    "icqq": "node lib/tools/icqq.js"
  },
  "dependencies": {
    "@icqqjs/icqq": "latest",
    "@inquirer/prompts": "5.1.2"
  },
  "devDependencies": {
    "@types/express": "latest",
    "@types/lodash": "latest",
    "@types/node": "latest",
    "@types/node-schedule": "latest",
    "@types/ws": "latest",
    "eslint": "latest",
    "neostandard": "latest",
    "tsc-alias": "latest",
    "tsx": "latest",
    "typescript": "latest"
  },
  "peerDependenciesMeta": {
    "@icqqjs/icqq": {
      "optional": true
    }
  },
  "karin": {
    "apps": [],
    "outDir": "lib"
  },
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "LICENSE",
    "package.json",
    "README.md"
  ]
}
