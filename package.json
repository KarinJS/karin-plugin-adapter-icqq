{
  "name": "@karinjs/adapter-icqq",
  "version": "1.5.8",
  "description": "icqq adapter",
  "license": "MIT",
  "author": "karin",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsup",
    "icqq": "node dist/tools/icqq.js",
    "pub": "npm publish --access public",
    "sort": "sort-package-json",
    "karin": "karin",
    "dev": "tsx src/app.ts"
  },
  "devDependencies": {
    "@types/node": "^20.19.0",
    "eslint": "^9.28.0",
    "icqq": "npm:@icqqjs/icqq@^1.6.6",
    "neostandard": "^0.12.1",
    "node-karin": "^1.10.5",
    "tsup": "^8.5.0",
    "tsx": "^4.20.2",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "icqq": ">=0.6.10"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "development": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./icqq": {
      "types": "./dist/icqq.d.ts",
      "development": "./src/icqq.ts",
      "default": "./dist/icqq.js"
    }
  },
  "karin": {
    "main": "src/index.ts",
    "ts-apps": [
      "src/apps"
    ],
    "apps": [
      "dist/apps"
    ],
    "files": [
      "config",
      "data",
      "resources"
    ],
    "include": [
      "src/**/*"
    ]
  }
}